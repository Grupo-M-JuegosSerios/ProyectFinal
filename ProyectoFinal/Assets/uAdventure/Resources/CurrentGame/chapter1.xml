  <graph-conversation id="C1_MañanaDeReyes">
    <dialogue-node nodeindex="0" editor-x="50" editor-y="51">
      <speak-char idTarget="Madre">
        <resources name="No name" />
        <text>¡Buenos días, niños! ¿Habéis dormido bien?</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Hermano">
        <resources name="No name" />
        <text>¡¿Han llegado ya los Reyes?!</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Madre">
        <resources name="No name" />
        <text>¡Tranquilo, hijo! Desayunad primero, anda.</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Hermano">
        <resources name="No name" />
        <text>Este año me he portado bien, ¿verdad?</text>
      </speak-char>
      <condition />
      <speak-char idTarget="Madre">
        <resources name="No name" />
        <text>Por supuesto.</text>
      </speak-char>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>¿Les habrá llegado mi carta a los reyes?</text>
      </speak-player>
      <condition />
      <speak-char idTarget="Madre">
        <resources name="No name" />
        <text>¡Claro! Seguro que te han traído justo lo que querías.</text>
      </speak-char>
      <condition />
      <speak-player>
        <resources name="No name" />
        <text>¡¿De verdad?!</text>
      </speak-player>
      <condition />
      <speak-char idTarget="Madre">
        <resources name="No name" />
        <text>Por supuesto que sí, cariño.</text>
      </speak-char>
      <condition />
      <end-conversation />
    </dialogue-node>
  </graph-conversation>
  <graph-conversation id="C2_RegaloReyes">
    <dialogue-node nodeindex="0" editor-x="50" editor-y="50">
      <speak-char idTarget="Padre">
        <resources name="No name" />
        <text>¿Listos para abrir los regalos?</text>
      </speak-char>
      <condition />
      <child nodeindex="1" />
    </dialogue-node>
    <option-node nodeindex="1" editor-x="285" editor-y="50" x="285" y="50">
      <speak-player>
        <text>¡Sí!</text>
        <resources name="No name">
          <asset type="image" uri="" />
        </resources>
      </speak-player>
      <condition />
      <child nodeindex="2" />
      <speak-player>
        <text>No…</text>
        <resources name="No name" />
      </speak-player>
      <condition />
      <child nodeindex="3" />
    </option-node>
    <dialogue-node nodeindex="2" editor-x="505" editor-y="50">
      <end-conversation>
        <effect>
          <trigger-scene idTarget="MinijuegoReyes" x="400" y="450" transitionTime="300" transitionType="5" />
          <condition />
        </effect>
      </end-conversation>
    </dialogue-node>
    <dialogue-node nodeindex="3" editor-x="505" editor-y="244">
      <speak-char idTarget="Padre">
        <resources name="No name" />
        <text>Oh, entonces supongo que podemos esperar un poco más.</text>
